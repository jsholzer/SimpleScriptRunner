<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageVersion>1.0.8</PackageVersion>
        <Title>Database Scripts Runner</Title>
        <Authors>BuckTheOdds LLC</Authors>
        <Description>Library for running database scripts across different evnironments for easier DevOps</Description>
        <Copyright>Copyright 2018-2020</Copyright>
        <PackageProjectUrl>https://github.com/ericraider33/SimpleScriptRunner</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/ericraider33/SimpleScriptRunner/blob/master/LICENSE.md</PackageLicenseUrl>
        <PackageTags>MySQL Database Migrations Scripts Version DevOps CommandLine</PackageTags>
        <PackageReleaseNotes>Modified directory scan to sort results to be consistent between win/Linux</PackageReleaseNotes>
        <TargetFrameworks>net472;netcoreapp3.1</TargetFrameworks>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="MySql.Data" Version="8.0.15" />
    </ItemGroup>

</Project>
